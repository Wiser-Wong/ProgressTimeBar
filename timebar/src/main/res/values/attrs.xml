<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 万能进度条-->
    <declare-styleable name="ProgressTimeBar">
        <attr name="progressTimeMode" format="enum">
            <enum name="NO_HAS_TIME" value="0" />
            <enum name="HAS_TIME" value="1" />
        </attr>
        <attr name="progressTimeColor" format="color|reference" />
        <attr name="progressUnPlayColor" format="color|reference" />
        <attr name="progressPlayColor" format="color|reference" />
        <attr name="progressBufferColor" format="color|reference" />
        <attr name="progressBarColor" format="color|reference" />
        <attr name="progressBarShadowColor" format="color|reference" />
        <attr name="progressBarSrc" format="reference" />
        <attr name="progressUnPlaySrc" format="reference" />
        <attr name="progressBufferSrc" format="reference" />
        <attr name="progressPlaySrc" format="reference" />
        <attr name="progressIsBarShadow" format="boolean" />
        <attr name="progressIsHasBar" format="boolean" />
        <attr name="progressIsHasBuffer" format="boolean" />
        <attr name="progressBarShadowPadding" format="dimension|reference" />
        <attr name="progressTimeTextSize" format="dimension|reference" />
        <attr name="progressTimePadding" format="dimension|reference" />
        <attr name="progressHeight" format="dimension|reference" />
        <attr name="progressBarHeight" format="dimension|reference" />
        <attr name="progressRoundRadius" format="dimension|reference" />
        <attr name="progressUnPlayStartColor" format="color|reference" />
        <attr name="progressUnPlayCenterColor" format="color|reference" />
        <attr name="progressUnPlayEndColor" format="color|reference" />
        <attr name="progressBufferStartColor" format="color|reference" />
        <attr name="progressBufferCenterColor" format="color|reference" />
        <attr name="progressBufferEndColor" format="color|reference" />
        <attr name="progressPlayStartColor" format="color|reference" />
        <attr name="progressPlayCenterColor" format="color|reference" />
        <attr name="progressPlayEndColor" format="color|reference" />
        <attr name="progressIsHasPlayProgress" format="boolean" />
        <attr name="progressIsCanClick" format="boolean" />
        <attr name="progressMaxDuration" format="integer" />
        <attr name="progressCurrentDuration" format="integer" />
        <attr name="progressBufferDuration" format="integer" />
    </declare-styleable>

    <!-- 万能进度条-->
    <declare-styleable name="ProgressValueBar">
        <attr name="progressValueUnPlayColor" format="color|reference" />
        <attr name="progressValuePlayColor" format="color|reference" />
        <attr name="progressValueBarColor" format="color|reference" />
        <attr name="progressValueBarShadowColor" format="color|reference" />
        <attr name="progressValueBarSrc" format="reference" />
        <attr name="progressValueUnPlaySrc" format="reference" />
        <attr name="progressValuePlaySrc" format="reference" />
        <attr name="progressValueIsBarShadow" format="boolean" />
        <attr name="progressValueIsHasBar" format="boolean" />
        <attr name="progressValueBarShadowPadding" format="dimension|reference" />
        <attr name="progressValueHeight" format="dimension|reference" />
        <attr name="progressValueBarHeight" format="dimension|reference" />
        <attr name="progressValueRoundRadius" format="dimension|reference" />
        <attr name="progressValueUnPlayStartColor" format="color|reference" />
        <attr name="progressValueUnPlayCenterColor" format="color|reference" />
        <attr name="progressValueUnPlayEndColor" format="color|reference" />
        <attr name="progressValuePlayStartColor" format="color|reference" />
        <attr name="progressValuePlayCenterColor" format="color|reference" />
        <attr name="progressValuePlayEndColor" format="color|reference" />
        <attr name="progressMaxValue" format="integer" />
        <attr name="progressCurrentValue" format="integer" />
        <attr name="progressStartValue" format="integer" />
        <attr name="progressValueIsHasPlayProgress" format="boolean" />
        <attr name="progressValueIsCanClick" format="boolean" />
    </declare-styleable>

    <!-- 颜色分割进度条-->
    <declare-styleable name="ProgressValueColor">
        <attr name="progressValueColorHeight" format="dimension|reference" />
        <attr name="progressValueColorRoundRadius" format="dimension|reference" />
        <attr name="progressValueColorBackgroundColor" format="color|reference" />
        <attr name="progressValueColorMaxValue" format="float" />
    </declare-styleable>

    <!-- 进度内文本进度条-->
    <declare-styleable name="ProgressInsideText">
        <attr name="progressInsideUnPlayColor" format="color|reference" />
        <attr name="progressInsidePlayColor" format="color|reference" />
        <attr name="progressInsideRoundRadius" format="dimension|reference" />
        <attr name="progressInsideUnPlayStartColor" format="color|reference" />
        <attr name="progressInsideUnPlayCenterColor" format="color|reference" />
        <attr name="progressInsideUnPlayEndColor" format="color|reference" />
        <attr name="progressInsidePlayStartColor" format="color|reference" />
        <attr name="progressInsidePlayCenterColor" format="color|reference" />
        <attr name="progressInsidePlayEndColor" format="color|reference" />
        <attr name="progressIsInsideTextWrapWidth" format="boolean" />
        <attr name="progressInsideText" format="string" />
        <attr name="progressInsideTextSize" format="dimension|reference" />
        <attr name="progressInsideTextColor" format="color|reference" />
        <attr name="progressInsideTextPadding" format="dimension|reference" />
        <attr name="progressInsideTextPaddingStart" format="dimension|reference" />
        <attr name="progressInsideTextPaddingTop" format="dimension|reference" />
        <attr name="progressInsideTextPaddingEnd" format="dimension|reference" />
        <attr name="progressInsideTextPaddingBottom" format="dimension|reference" />
        <attr name="progressInsideIsRadius" format="boolean" />
        <attr name="progressInsideMaxProgress" format="integer" />
        <attr name="progressInsideCurrentProgress" format="integer" />
    </declare-styleable>

</resources>